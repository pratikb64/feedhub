{
    "projectId": "feedhub",
    "projectName": "feedhub",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "prod",
            "name": "prod",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "projects",
            "$permissions": [],
            "databaseId": "prod",
            "name": "projects",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "domain",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                },
                {
                    "key": "owner",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                },
                {
                    "key": "teamId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                }
            ],
            "indexes": []
        },
        {
            "$id": "comments",
            "$permissions": [
                "create(\"users\")"
            ],
            "databaseId": "prod",
            "name": "comments",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "pathname",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 500,
                    "default": null
                },
                {
                    "key": "message",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "project",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                },
                {
                    "key": "positionX",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": -9223372036854775808,
                    "max": 9223372036854775807,
                    "default": null
                },
                {
                    "key": "positionY",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": -9223372036854775808,
                    "max": 9223372036854775807,
                    "default": null
                },
                {
                    "key": "xPath",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 500,
                    "default": null
                },
                {
                    "key": "innerWidth",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": -9223372036854775808,
                    "max": 9223372036854775807,
                    "default": null
                },
                {
                    "key": "owner",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "project",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "project"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        }
    ],
    "functions": [
        {
            "$id": "create-project",
            "name": "create-project",
            "runtime": "node-18.0",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/create-project"
        },
        {
            "$id": "invite-user",
            "name": "invite-user",
            "runtime": "node-18.0",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/invite-user"
        },
        {
            "$id": "check-user-exists",
            "name": "check-user-exists",
            "runtime": "node-18.0",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "users.read"
            ],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/check-user-exists"
        }
    ]
}